<div class="column column-75">
  <h3>
    My Feed
  </h3>
  <ul></ul>
  <% @articles.each do |a| %>
    <li>
      <span>
        <image class="favicon" src="<%= a.source.favicon %>"></image>
      </span>
      <span>
        <%= a.published_at.strftime('%m/%d/%Y') if a.published_at %>
      </span>
      <a href="#" data-body=<%= h(a.body) %>  data-micromodal-trigger="articleModalLong" data-title=<%= a.title %> data-url=<%= a.url %>>
        <%= a.title %>
      </a>
    </li>
  <% end %>
</div>
