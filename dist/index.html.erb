<div class="col col-9">
  <h3>
    My Feed
  </h3>
  <ul></ul>
  <% @articles.each do |a| %>
    <li>
      <span>
        <image class="favicon" src="<%= a.source.favicon %>"></image>
      </span>
      <%= a.published_at.strftime('%m/%d/%Y') if a.published_at %>
      <a data-body="<%= a.body.html_safe %>" data-custom-open= "#articleModalLong" data-title="<%= a.title %>" data-url="<%= a.url %>" href="#">
        <%= a.title %>
      </a>
    </li>
  <% end %>
</div>
